{
  "name": "stencil-calendar",
  "version": "0.0.1",
  "description": "Stencil Component Starter",
  "main": "dist/index.cjs.js",
  "module": "dist/custom-elements/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/custom-elements/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/stencil-calendar/stencil-calendar.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e --coverage",
    "unitTest": "stencil test --spec",
    "e2eTest": "stencil test --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "init.wc": "ln -sf ../Makefile . && make init-wc && unlink Makefile",
    "lint": "eslint src/**/*{.ts,.tsx}"
  },
  "dependencies": {
    "@stencil/core": "^2.21.0",
    "@tec-registry/daisy": "^1.0.1",
    "@tec-registry/nest-notification-modal-dialog": "^1.0.4",
    "dotenv": "^16.0.3"
  },
  "devDependencies": {
    "@stencil/eslint-plugin": "^0.4.0",
    "@stencil/sass": "^2.0.3",
    "@types/jest": "^27.0.3",
    "@types/node": "^16.18.11",
    "jest": "^27.4.5",
    "jest-cli": "^27.4.5",
    "puppeteer": "^10.0.0"
  },
  "license": "MIT",
  "publishConfig": {
    "@tec-registry:registry": "https://git.energytech-global.com/api/v4/projects/102/packages/npm/"
